{
  "name": "unified-ai-hub",
  "version": "1.0.0",
  "description": "Unified AI Hub - Multi-provider AI orchestration platform",
  "main": "dist/backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon --watch backend --ext ts --exec ts-node backend/server.ts",
    "dev:frontend": "cd frontend && vite",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc -p backend/tsconfig.json",
    "build:frontend": "cd frontend && vite build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "migrate": "typeorm migration:run -d backend/config/data-source.ts",
    "migrate:create": "typeorm migration:create",
    "migrate:revert": "typeorm migration:revert -d backend/config/data-source.ts",
    "seed": "ts-node backend/scripts/seed.ts",
    "start": "node dist/backend/server.js",
    "electron:dev": "electron .",
    "electron:build": "electron-builder"
  },
  "keywords": [
    "ai",
    "chatgpt",
    "claude",
    "gemini",
    "orchestration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@qdrant/js-client-rest": "^1.8.2",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-forge": "^1.3.1",
    "pg": "^8.11.3",
    "rate-limiter-flexible": "^5.0.0",
    "reflect-metadata": "^0.2.1",
    "sharp": "^0.33.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.25",
    "@types/node-forge": "^1.3.11",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "concurrently": "^8.2.2",
    "electron": "^29.1.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
